// Server resource script â€” version info for Windows executable metadata
#include "winres.h"
#include "../Dependencies/Shared/version_rc.h"

/////////////////////////////////////////////////////////////////////////////
// Version
//
VS_VERSION_INFO VERSIONINFO
    FILEVERSION    VER_SERVER_FILEVERSION
    PRODUCTVERSION VER_SERVER_FILEVERSION
    FILEFLAGSMASK  VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
    FILEFLAGS      VS_FF_DEBUG
#else
    FILEFLAGS      0x0L
#endif
    FILEOS         VOS_NT_WINDOWS32
    FILETYPE       VFT_APP
    FILESUBTYPE    VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0"
        BEGIN
            VALUE "CompanyName",      "Helbreath Project"
            VALUE "FileDescription",  "Helbreath Server"
            VALUE "FileVersion",      VER_SERVER_FULL
            VALUE "InternalName",     "HelbreathServer"
            VALUE "OriginalFilename", "Server.exe"
            VALUE "ProductName",      "Helbreath"
            VALUE "ProductVersion",   VER_SERVER_DISPLAY
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200
    END
END
